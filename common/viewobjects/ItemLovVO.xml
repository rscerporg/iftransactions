<ViewObject
   Name="ItemLovVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="oracle.apps.po.rscapps.oss.iftransactions.common.viewobjects.ItemLovVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select tl.inventory_item_id, b.segment1, tl.description, tl.long_description
from MTL_SYSTEM_ITEMS_TL tl, mtl_system_items_b b
where tl.ORGANIZATION_ID = 114
and b.ORGANIZATION_ID = 114
and tl.inventory_item_id = b.inventory_item_id
and b.enabled_FLAG = 'Y'
   ]]></SQLQuery>
